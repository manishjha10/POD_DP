class Solution {
    public long zeroFilledSubarray(int[] nums) {
        long ans  = 0; 
        long subarray = 0; 

        for(int ele :  nums)
        {
            if(ele == 0)
            {
                subarray ++ ;
                ans  += subarray; 
            }
            else 
            {
                subarray = 0;
            }
        }
        return ans ;
    }
}
